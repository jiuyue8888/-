.tabs{
    position: fixed;
    left: 0;
    bottom: 0;
    z-index: 4;
    width: 100%;
    height: 160rpx;
    box-sizing: border-box;
    padding: 40rpx 20rpx;
    font-size: 28rpx;
    background: #190202;
}
.tabs navigator{
position:relative;
    float: left;
    width: 33.33%;
    text-align: center;
    color:#fff;
    text-decoration: none;
    outline-width: 0;
    -webkit-tap-highlight-color: transparent;
    -webkit-touch-callout: none;
    -webkit-user-select: none;
    user-select:none;
}
.tabs navigator image{
    width:90rpx;
    height:90rpx;
    margin:0 auto;
}
.tabs navigator text{
    position:absolute;
    left:50%;
    bottom:0;
    width:90rpx;
    height:40rpx;
    margin-left:-45rpx;
    font-size:24rpx;
    line-height:40rpx;
    text-align:center;

}
.tabs navigator.curr{
    color: #c63835;
}