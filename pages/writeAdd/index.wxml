<view class="submit-order">
    <image src="../../images/submitbg.jpg" class="bg" />
    <view class="writeAdd">
        <radio-group class="radio-group" bindchange="radioChange">
            <radio class="radio" wx:for-items="{{items}}" wx:key="name" checked="{{item.checked}}" value="{{index}}">
                <text>{{item.value}}</text>
                <image src="../../images/editor.png" class="editor" bindtap="gotoUrlAddress" data-type="{{item.value}}"/>
            </radio>
            
        </radio-group>
    </view>
    <view class="editorBtn">
        <navigator url="">
            <image src="../../images/addwx.png" class="add-btn" />
        </navigator>
        <image src="../../images/addnew.png" class="add-btn" bindtap="gotoUrlAddress" />
    </view>
    <tab id="comp" />
</view>